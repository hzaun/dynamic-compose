<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nuzharukiya.dynamiccompose">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DynamicCompose">
        <activity
            android:name="com.nuzharukiya.dynamiccompose.ui.ComplexPositionActivity"
            android:exported="true"
            android:label="@string/title_activity_position"
            android:theme="@style/Theme.DynamicCompose.NoActionBar" />
        <activity
            android:name="com.nuzharukiya.dynamiccompose.ui.PositionActivity"
            android:exported="true"
            android:label="@string/title_activity_position"
            android:theme="@style/Theme.DynamicCompose.NoActionBar" />
        <activity
            android:name="com.nuzharukiya.dynamiccompose.ui.UiActivity"
            android:exported="true"
            android:label="@string/title_activity_ui"
            android:theme="@style/Theme.DynamicCompose.NoActionBar" />
        <activity
            android:name="com.nuzharukiya.dynamiccompose.ui.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DynamicCompose.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>